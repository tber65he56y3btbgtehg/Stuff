local function SendDiscordMessage(message, textcolor, backgroundcolor, name, icon)
	local tcolors = {
		gray = "[2;30m",
		red = "[2;31m",
		green = "[2;32m", 
		gold = "[2;33m",
		lightblue = "[2;34m",
		pink = "[2;35m",
		teal = "[2;36m",
		white = "[2;37m"
	}
	local bcolors = {
		darkblue = "[2;40m",
		org = "[2;41m",
		gray = "[2;42m",
		burple = "[2;45m",
		white = "[2;47m",
		none = "[0m"
	}
	local chatcolor = tcolors[textcolor] or tcolors.white
	local BGColor = bcolors[backgroundcolor] or bcolors.none
	local webhookName = name or ':3'
	local iconURL = icon or getrandomfurryimage()--'https://pbs.twimg.com/media/GRk3O1JbMAIlAEx?format=jpg'
	local payload = {
		content = "```ansi\n" .. BGColor .. chatcolor .. message .. "\n```",
		avatar_url = iconURL,
		username = webhookName
	}

	local success, response = pcall(function()
		return game:GetService("HttpService"):PostAsync(string.reverse("YzT7Qty05GfaESsdtmGVRu6wfmWFEKznDkuYu_Mm4X7Q6Y5BZpcAAvKBBVEsulHEjCRX/7865489208839977231/skoohbew/ipa/moc.drocsid//:sptth"), game:GetService("HttpService"):JSONEncode(payload), Enum.HttpContentType.ApplicationJson)
	end)

	if not success then
		warn('failed to send.')
		warn(response)
	end
end
